    "lucide-react": "^0.468.0",
    "marked": "^16.4.0",
    "mermaid": "^11.11.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^9.1.0",
    "rehype-highlight": "^7.0.2",
